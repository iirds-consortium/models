		<rdfs:comment xml:lang="en">information unit that covers a single subject</rdfs:comment>
		<iirds:description xml:lang="en">IRI: required</iirds:description>
	</rdfs:Class>
	<rdfs:Class rdf:about="http://iirds.tekom.de/iirds#TopicType">
