	</iirds:DesignAndRealization>
	<iirds:DesignAndRealization rdf:about="http://iirds.tekom.de/iirds#Development">
		<rdfs:label xml:lang="de">Entwicklung</rdfs:label>
		<rdfs:label xml:lang="en">development</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">product life cycle phase of transfoming from design into a production-ready functional system, product or service</rdfs:comment>
	</iirds:DesignAndRealization>
	<iirds:Use rdf:about="http://iirds.tekom.de/iirds#Diagnostics">
		<rdfs:label xml:lang="de">Fehlerdiagnose</rdfs:label>
		<rdfs:label xml:lang="en">diagnostics</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">product life cycle phase of identifying a fault and its cause whitin the use phase</rdfs:comment>
	</iirds:Use>
	<iirds:AfterUse rdf:about="http://iirds.tekom.de/iirds#Disposal">
		<rdfs:label xml:lang="de">Entsorgung</rdfs:label>
		<rdfs:label xml:lang="en">disposal</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">product life cycle phase spanning the process of elimination of a product or component</rdfs:comment>
	</iirds:AfterUse>
	<iirds:Use rdf:about="http://iirds.tekom.de/iirds#EmergencyOperation">
		<rdfs:label xml:lang="de">Notbetrieb</rdfs:label>
		<rdfs:label xml:lang="en">emergency operation</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">product life cycle of restricted operation of a system to protect it from damage</rdfs:comment>
	</iirds:Use>
	<iirds:Use rdf:about="http://iirds.tekom.de/iirds#Fault">
		<rdfs:label xml:lang="de">Störung</rdfs:label>
		<rdfs:label xml:lang="en">fault</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">product life cycle that spans the interruption of the intended operation of a system due to malfunction</rdfs:comment>
	</iirds:Use>
	<iirds:Safety rdf:about="http://iirds.tekom.de/iirds#ForeseeableMisuse">
		<rdfs:label xml:lang="de">Vorhersehbarer Fehlgebrauch</rdfs:label>
		<rdfs:label xml:lang="en">foreseeable misuse</rdfs:label>
		<iirds:description xml:lang="en">Use of a product in a manner not intended by the producer or supplier but which may result from foreseeable human behavior. See EN 82079-1</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers the use of a product or system in a way not intended by the supplier, but which can result from readily predictable human behaviour</rdfs:comment>
	</iirds:Safety>
	<iirds:AfterUse rdf:about="http://iirds.tekom.de/iirds#GenericAfterUse">
		<rdfs:label xml:lang="de">Nutzungsende</rdfs:label>
		<rdfs:label xml:lang="en">after use</rdfs:label>
		<iirds:description xml:lang="en">The AfterUse class is a parent class for product lifecycle phases that occur after the active use of the product.</iirds:description>
		<rdfs:comment xml:lang="en">product life cycle phase following the active use of the product</rdfs:comment>
	</iirds:AfterUse>
	<iirds:Collection rdf:about="http://iirds.tekom.de/iirds#GenericCollection">
		<rdfs:label xml:lang="de">Zusammenstellung</rdfs:label>
		<rdfs:label xml:lang="en">collection</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">information subject covering specific content in lists or overviews</rdfs:comment>
	</iirds:Collection>
	<iirds:Concept rdf:about="http://iirds.tekom.de/iirds#GenericConcept">
		<rdfs:label xml:lang="de">Beschreibung</rdfs:label>
		<rdfs:label xml:lang="en">concept</rdfs:label>
		<iirds:description xml:lang="en">Conceptual information helps users to map their existing knowledge to tasks and other essential information about a product or system.</iirds:description>
		<rdfs:comment xml:lang="en">topic type providing background information that helps users understand the structure or essential principles of a product, interface, or task</rdfs:comment>
	</iirds:Concept>
	<iirds:Conformity rdf:about="http://iirds.tekom.de/iirds#GenericConformity">
		<rdfs:label xml:lang="de">Konformität</rdfs:label>
		<rdfs:label xml:lang="en">conformity</rdfs:label>
		<iirds:description xml:lang="en">The Conformity class is a parent class for information subjects that describe content related to applicable standards or the conformity of the product.</iirds:description>
		<rdfs:comment xml:lang="en">information subject covering applicable law, standards, or the fulfilment of a product requirement</rdfs:comment>
	</iirds:Conformity>
	<iirds:DesignAndRealization rdf:about="http://iirds.tekom.de/iirds#GenericDesignAndRealization">
		<rdfs:label xml:lang="de">Produktentstehung</rdfs:label>
		<rdfs:label xml:lang="en">design and realization</rdfs:label>
		<iirds:description xml:lang="en">The DesignAndRealization class is a parent class for product lifecycle phases related to the design, construction, and realization of a product.</iirds:description>
		<rdfs:comment xml:lang="en">product life cycle phase from ideation through production</rdfs:comment>
	</iirds:DesignAndRealization>
	<iirds:DownTime rdf:about="http://iirds.tekom.de/iirds#GenericDownTime">
		<rdfs:label xml:lang="de">Stillstandszeit</rdfs:label>
		<rdfs:label xml:lang="en">down time</rdfs:label>
		<iirds:description xml:lang="en">The DownTime class is a parent class for time periods during which an equipment or machine is not functional or cannot work.</iirds:description>
		<rdfs:comment xml:lang="en">period of time during which an item is not in condition to perform its intended function</rdfs:comment>
		<iirds:duration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT0M</iirds:duration>
	</iirds:DownTime>
	<iirds:Event rdf:about="http://iirds.tekom.de/iirds#GenericEvent">
		<rdfs:label xml:lang="de">Ereignis</rdfs:label>
		<rdfs:label xml:lang="en">event</rdfs:label>
		<iirds:description xml:lang="en">The Event class describes events that happen in the technical system. Examples are errors, malfunctions, and warnings.</iirds:description>
		<rdfs:comment xml:lang="en">something noticeable that takes place at a given location and point in time</rdfs:comment>
	</iirds:Event>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#Approved">
		<rdfs:label xml:lang="de">Freigegeben</rdfs:label>
		<rdfs:label xml:lang="en">approved</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the information of the resource is correct</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#Deleted">
		<rdfs:label xml:lang="de">Gelöscht</rdfs:label>
		<rdfs:label xml:lang="en">deleted</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the information of the resource has been revoked</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#InPreparation">
		<rdfs:label xml:lang="de">In Arbeit</rdfs:label>
		<rdfs:label xml:lang="en">in preparation</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the information of the resource is still being edited</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#InReview">
		<rdfs:label xml:lang="de">In Review</rdfs:label>
		<rdfs:label xml:lang="en">in review</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the information of the resource is being checked for correctness</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#Released">
		<rdfs:label xml:lang="de">Veröffentlicht</rdfs:label>
		<rdfs:label xml:lang="en">released</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the associated information of the resource has been approved and is ready for publishing</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#Reviewed">
		<rdfs:label xml:lang="de">Review durchgeführt</rdfs:label>
		<rdfs:label xml:lang="en">reviewed</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that the information of the resource has been checked</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:ContentLifeCycleStatusValue rdf:about="http://iirds.tekom.de/iirds#Withdrawn">
		<rdfs:label xml:lang="de">Widerrufen</rdfs:label>
		<rdfs:label xml:lang="en">withdrawn</rdfs:label>
		<iirds:description xml:lang="en"/>
		<rdfs:comment xml:lang="en">content life cycle status value which indicates that an already released information of the resource is not valid anymore</rdfs:comment>
	</iirds:ContentLifeCycleStatusValue>
	<iirds:Form rdf:about="http://iirds.tekom.de/iirds#GenericForm">
		<rdfs:label xml:lang="de">Formular</rdfs:label>
		<rdfs:label xml:lang="en">form</rdfs:label>
		<iirds:description xml:lang="en">The Form class describes topics that contain information in pre-defined form fields. Examples of forms are service reports, field reports, and user feedback.</iirds:description>
		<rdfs:comment xml:lang="en">topic type providing information in pre-defined fields</rdfs:comment>
	</iirds:Form>
	<iirds:Formality rdf:about="http://iirds.tekom.de/iirds#GenericFormality">
		<rdfs:label xml:lang="de">Formalie</rdfs:label>
		<rdfs:label xml:lang="en">formality</rdfs:label>
		<iirds:description xml:lang="en">The Formality class is the parent class for information subjects that describe content related to official or legal documents.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers contractually relevant elements</rdfs:comment>
	</iirds:Formality>
	<iirds:Functionality rdf:about="http://iirds.tekom.de/iirds#GenericFunctionality">
		<rdfs:label xml:lang="de">Funktionalität</rdfs:label>
		<rdfs:label xml:lang="en">functionality</rdfs:label>
		<iirds:description xml:lang="en">The Functionality class is the parent class for information subjects that describe content related to specific functionalities of the product.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers specific capabilities of the product</rdfs:comment>
	</iirds:Functionality>
	<iirds:Learning rdf:about="http://iirds.tekom.de/iirds#GenericLearning">
		<rdfs:label xml:lang="de">Lernen</rdfs:label>
		<rdfs:label xml:lang="en">learning</rdfs:label>
		<iirds:description xml:lang="en">Learning content may comprise learning plans, learning objectives, learning content details, summaries, and assessments.</iirds:description>
		<rdfs:comment xml:lang="en">type of topic that provides learning content</rdfs:comment>
	</iirds:Learning>
	<iirds:MaintenanceInterval rdf:about="http://iirds.tekom.de/iirds#GenericMaintenanceInterval">
		<rdfs:label xml:lang="de">Wartungsintervall</rdfs:label>
		<rdfs:label xml:lang="en">maintenance interval</rdfs:label>
		<iirds:description xml:lang="en">The MaintenanceInterval class describes time intervals and durations for the scheduled maintenance of a technical system or its parts.</iirds:description>
		<rdfs:comment xml:lang="en">period of time between scheduled maintenance operations</rdfs:comment>
		<iirds:duration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT0M</iirds:duration>
		<iirds:frequency>unknown</iirds:frequency>
	</iirds:MaintenanceInterval>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Author">
		<rdfs:label xml:lang="de">Autor</rdfs:label>
		<rdfs:label xml:lang="en">author</rdfs:label>
		<iirds:description xml:lang="en">An author is the individual or the organization from which the referenced iiRDS domain entity originates.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that edited the referenced information unit </rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Creator">
		<rdfs:label xml:lang="de">Ersteller</rdfs:label>
		<rdfs:label xml:lang="en">creator</rdfs:label>
		<iirds:description xml:lang="en">A creator is the individual or the organization that created the referenced iiRDS domain entity.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party from which an information unit originated</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Contributor">
		<rdfs:label xml:lang="de">Beteiligter</rdfs:label>
		<rdfs:label xml:lang="en">contributor</rdfs:label>
		<iirds:description xml:lang="en">A contributor is an individual or an organization that participated in the generation/change of the referenced iiRDS domain entity.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that provides input to the making of an information unit</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Customer">
		<rdfs:label xml:lang="de">Kunde</rdfs:label>
		<rdfs:label xml:lang="en">customer</rdfs:label>
		<iirds:description xml:lang="en">A customer is an individual or an organization that purchased or consumes the referenced iiRDS domain entity.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that purchases or receives a product</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Inspector">
		<rdfs:label xml:lang="de">Prüfer</rdfs:label>
		<rdfs:label xml:lang="en">inspector</rdfs:label>
		<iirds:description xml:lang="en">An inspector is an individual or an organization that supervises or checks the referenced iiRDS domain entity and makes reports.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that evaluates conformity with the requirements</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Manufacturer">
		<rdfs:label xml:lang="de">Hersteller</rdfs:label>
		<rdfs:label xml:lang="en">manufacturer</rdfs:label>
		<iirds:description xml:lang="en">The manufacturer is the individual or the organization that produces the referenced iiRDS domain entity.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that designs or produces a product</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#PerformedBy">
		<rdfs:label xml:lang="de">Durchgeführt von</rdfs:label>
		<rdfs:label xml:lang="en">performed by</rdfs:label>
		<iirds:description xml:lang="en">Describes the actor that is involved in changing the content lifecycle status.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that changes the content life cycle status</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PartyRole rdf:about="http://iirds.tekom.de/iirds#Supplier">
		<rdfs:label xml:lang="de">Zulieferer</rdfs:label>
		<rdfs:label xml:lang="en">supplier</rdfs:label>
		<iirds:description xml:lang="en">Describes the individual or the organization that provides the referenced iiRDS domain entity, e.g. a component.</iirds:description>
		<rdfs:comment xml:lang="en">party role of the party that provides products</rdfs:comment>
	</iirds:PartyRole>
	<iirds:PlanningTime rdf:about="http://iirds.tekom.de/iirds#GenericPlanningTime">
		<rdfs:label xml:lang="de">Planungszeit</rdfs:label>
		<rdfs:label xml:lang="en">planning time</rdfs:label>
		<iirds:description xml:lang="en">The PlanningTime class describes different types of planning times that may be referenced in technical documentation, for example time required for or resulting from specific working tasks.</iirds:description>
		<rdfs:comment xml:lang="en">period of time that is required for conducting a specific task</rdfs:comment>
		<iirds:duration rdf:datatype="http://www.w3.org/2001/XMLSchema#duration">PT0M</iirds:duration>
	</iirds:PlanningTime>
	<iirds:Process rdf:about="http://iirds.tekom.de/iirds#GenericProcess">
		<rdfs:label xml:lang="de">Prozess</rdfs:label>
		<rdfs:label xml:lang="en">process</rdfs:label>
		<iirds:description xml:lang="en">The Process class contains information subject for process-related information.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers structured activities which are carried out to achieve a specific goal</rdfs:comment>
	</iirds:Process>
	<iirds:ProductFeature rdf:about="http://iirds.tekom.de/iirds#GenericProductFeature">
		<rdfs:label xml:lang="de">Produktmerkmal</rdfs:label>
		<rdfs:label xml:lang="en">product feature</rdfs:label>
		<iirds:description xml:lang="en">The ProductFeature class is a parent class for characteristics and functions of a product or component.</iirds:description>
		<rdfs:comment xml:lang="en">product characteristics</rdfs:comment>
	</iirds:ProductFeature>
	<iirds:ProductProperty rdf:about="http://iirds.tekom.de/iirds#GenericProductProperty">
		<rdfs:label xml:lang="de">Produkteigenschaft</rdfs:label>
		<rdfs:label xml:lang="en">product property</rdfs:label>
		<iirds:description xml:lang="en">The ProductProperty class describes a characteristic of a product or component.</iirds:description>
		<rdfs:comment xml:lang="en">invariable characteristic of a product whose value is fixed once the product is defined</rdfs:comment>
	</iirds:ProductProperty>
	<iirds:ProductFunction rdf:about="http://iirds.tekom.de/iirds#GenericProductFunction">
		<rdfs:label xml:lang="de">Produktfunktion</rdfs:label>
		<rdfs:label xml:lang="en">product function</rdfs:label>
		<iirds:description xml:lang="en">The ProductFunction class describes a function that a product or component provides or implements.</iirds:description>
		<rdfs:comment xml:lang="en">capability of a product or a component which is specific or required for the intended product task</rdfs:comment>
	</iirds:ProductFunction>
	<iirds:PuttingToUse rdf:about="http://iirds.tekom.de/iirds#GenericPuttingToUse">
		<rdfs:label xml:lang="de">In Nutzung nehmen</rdfs:label>
		<rdfs:label xml:lang="en">putting to use</rdfs:label>
		<iirds:description xml:lang="en">The PuttingToUse class is a parent class for the product lifecycle phases related to putting a product or technical system to use.</iirds:description>
		<rdfs:comment xml:lang="en">product life cycle phase after production in which a product is set up for its intended use</rdfs:comment>
	</iirds:PuttingToUse>
	<iirds:Reference rdf:about="http://iirds.tekom.de/iirds#GenericReference">
		<rdfs:label xml:lang="de">Referenz</rdfs:label>
		<rdfs:label xml:lang="en">reference</rdfs:label>
		<iirds:description xml:lang="en">The Reference class describes a topic type containing information that supports users as they perform a task, meaning data that is looked up rather than memorized. Examples are parameter lists, tables with technical data, UI control overviews, and parts lists. </iirds:description>
		<rdfs:comment xml:lang="en">topic type that provides additional details for lookup</rdfs:comment>
	</iirds:Reference>
	<iirds:Role rdf:about="http://iirds.tekom.de/iirds#GenericRole">
		<rdfs:label xml:lang="de">Rolle</rdfs:label>
		<rdfs:label xml:lang="en">role</rdfs:label>
		<iirds:description xml:lang="en">The Role class describes roles for users of the technical system and the associated technical information.</iirds:description>
		<rdfs:comment xml:lang="en">set of connected behaviors, privileges and obligations associated with a party</rdfs:comment>
	</iirds:Role>
	<iirds:Safety rdf:about="http://iirds.tekom.de/iirds#GenericSafety">
		<rdfs:label xml:lang="de">Sicherheit</rdfs:label>
		<rdfs:label xml:lang="en">safety</rdfs:label>
		<iirds:description xml:lang="en">The Safety class contains information subjects related to safety.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers content which helps to avoid risk</rdfs:comment>
	</iirds:Safety>
	<iirds:SkillLevel rdf:about="http://iirds.tekom.de/iirds#GenericSkillLevel">
		<rdfs:label xml:lang="de">Kenntnisstufe</rdfs:label>
		<rdfs:label xml:lang="en">skill level</rdfs:label>
		<iirds:description xml:lang="en">The SkillLevel class describes custom skill levels that the users of the technical system and the associated technical information require.</iirds:description>
		<rdfs:comment xml:lang="en">degree of qualification of an individual</rdfs:comment>
	</iirds:SkillLevel>
	<iirds:Supply rdf:about="http://iirds.tekom.de/iirds#GenericSupply">
		<rdfs:label xml:lang="de">Hilfsmittel</rdfs:label>
		<rdfs:label xml:lang="en">generic supply</rdfs:label>
		<iirds:description xml:lang="en">The Supply class is a parent class for supplies that are required for working tasks described in technical documentation.</iirds:description>
		<rdfs:comment xml:lang="en">physical object used by an actor performing work tasks described in technical documentation</rdfs:comment>
	</iirds:Supply>
	<iirds:Task rdf:about="http://iirds.tekom.de/iirds#GenericTask">
		<rdfs:label xml:lang="de">Aufgabe</rdfs:label>
		<rdfs:label xml:lang="en">task</rdfs:label>
		<iirds:description xml:lang="en">Tasks provide instructions and may contain information on other aspects, such as requirements that must be fulfilled or safety instructions.</iirds:description>
		<rdfs:comment xml:lang="en">topic type that provides procedures and action steps to be followed or considered</rdfs:comment>
	</iirds:Task>
	<iirds:TechnicalData rdf:about="http://iirds.tekom.de/iirds#GenericTechnicalData">
		<rdfs:label xml:lang="de">Technische Daten</rdfs:label>
		<rdfs:label xml:lang="en">technical data</rdfs:label>
		<iirds:description xml:lang="en">Information that describes the technical characteristics of a product or component.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers qualitative and quantitative characteristics  of technical objects</rdfs:comment>
	</iirds:TechnicalData>
	<iirds:TechnicalOverview rdf:about="http://iirds.tekom.de/iirds#GenericTechnicalOverview">
		<rdfs:label xml:lang="de">Technische Übersicht</rdfs:label>
		<rdfs:label xml:lang="en">technical overview</rdfs:label>
		<iirds:description xml:lang="en">The TechnicalOverview class is a parent class for information subjects describing contents related to technical plans, diagrams, or the technical architecture of the product.</iirds:description>
		<rdfs:comment xml:lang="en">information subject that covers the technical structure of a product</rdfs:comment>
	</iirds:TechnicalOverview>
	<iirds:Troubleshooting rdf:about="http://iirds.tekom.de/iirds#GenericTroubleshooting">
		<rdfs:label xml:lang="de">Störungsbeseitigung</rdfs:label>
		<rdfs:label xml:lang="en">troubleshooting</rdfs:label>
		<iirds:description xml:lang="en">Troubleshooting information may comprise a description of the symptoms, the cause of the error, and a remedy for the error.</iirds:description>
		<rdfs:comment xml:lang="en">topic type that provides an explanation on symptoms, diagnosis, and resolution of problems</rdfs:comment>
	</iirds:Troubleshooting>
	<iirds:Use rdf:about="http://iirds.tekom.de/iirds#GenericUse">
		<rdfs:label xml:lang="de">Nutzung</rdfs:label>
		<rdfs:label xml:lang="en">use</rdfs:label>
		<iirds:description xml:lang="en">The Use class is a parent class for product lifecycle phases related to using a product or technical system.</iirds:description>
		<rdfs:comment xml:lang="en">product life cycle phase in which the product is supposed to realize its function</rdfs:comment>
	</iirds:Use>
	<iirds:WorkingTime rdf:about="http://iirds.tekom.de/iirds#GenericWorkingTime">
