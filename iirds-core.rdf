		<schema:domainIncludes rdf:resource="http://iirds.tekom.de/iirds#ContentLifeCycleStatus"/>
		<schema:rangeIncludes rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
